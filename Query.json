        "users",
        "newUsers",
        "percentNewSessions",
        "sessionsPerUser"
      ],
      "dimensions": [
        "userType",
        "sessionCount",
        "daysSinceLastSession",
        "userDefinedValue",
        "userBucket"
      ]
    },
    {
      "name": "Session",
      "metrics": [
        "sessions",
