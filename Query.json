    },
    {
      "name": "Adwords",
      "metrics": [
        "impressions",
        "adClicks",
        "adCost",
        "CPM",
        "CPC",
        "CTR",
        "costPerTransaction",
        "costPerGoalConversion",
        "costPerConversion"
